{
    "name": "NHentai Downloader",
    "version": "1.0",
    "description": "A chrome/firefox extension to download doujinshi from NHentai",
    "icons": {
      "64": "Icon.png"
    },
    "browser_action": {
      "default_popup": "index.html"
    },
    "permissions": [
      "downloads",
      "tabs",
      "https://nhentai.net/*"
    ],
    "author": "Xwilarg",
    "manifest_version": 2
  }