{
    "name": "NHentai Downloader",
    "version": "1.3.0",
    "description": "A chrome extension to download doujinshi from NHentai",
    "icons": {
      "64": "Icon.png"
    },
    "browser_action": {
      "default_popup": "index.html"
    },
    "permissions": [
      "downloads",
      "tabs",
      "https://nhentai.net/*",
      "https://i.nhentai.net/*"
    ],
    "author": "Xwilarg",
    "background": {
      "scripts": ["js/jszip/dist/jszip.min.js", "js/FileSaver.js/dist/FileSaver.min.js", "js/background.js"]
    },
    "manifest_version": 2
  }